{
  "extends": "airbnb",
  "ecmaFeatures": {
    "experimentalObjectRestSpread": true
  },
  "globals": {
    "chrome": false,
    "componentHandler": false,
    "sinon": false,
  },
  "rules": {
    "react/jsx-no-bind": [2, {
      "ignoreRefs": true
    }],
    "prefer-template": 1,
    "no-use-before-define": 0
  }
}
